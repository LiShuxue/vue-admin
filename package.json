{
  "name": "vue-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "mock": "vue-cli-service serve --mode mock",
    "sit": "vue-cli-service serve --mode sit",
    "prod": "vue-cli-service serve --mode prod",
    "build-mock": "vue-cli-service build --mode mock",
    "build-sit": "vue-cli-service build --mode sit",
    "build-prod": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "start-mock": "yarn lint && yarn mock",
    "start-sit": "yarn lint && yarn sit",
    "start-prod": "yarn lint && yarn prod"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.2.1",
    "element-china-area-data": "^4.1.2",
    "element-ui": "^2.9.1",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "wangeditor": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "husky": "^3.0.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10"
  }
}
