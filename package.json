{
  "name": "vue-admin",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "mock": "vue-cli-service serve --mode mock",
    "sit": "vue-cli-service serve --mode sit",
    "prod": "vue-cli-service serve --mode prod",
    "build-mock": "vue-cli-service build --mode mock",
    "build-sit": "vue-cli-service build --mode sit",
    "build-prod": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "start-mock": "yarn lint && yarn mock",
    "start-sit": "yarn lint && yarn sit",
    "start-prod": "yarn lint && yarn prod"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.9.6",
    "echarts": "^4.9.0",
    "element-ui": "^2.14.1",
    "normalize.css": "^8.0.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0",
    "wangeditor": "^4.5.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.6",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "sass": "^1.37.5",
    "sass-loader": "^10.1.0",
    "vue-template-compiler": "^2.6.11"
  }
}
